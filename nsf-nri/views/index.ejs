    <!-- Page Content -->
    <div class="container" id = "#page-content-wrapper">

        <div class="row">
            <div class="col-lg-12 text-center">
                <h1>Task Planner for a Collaborative Robot </h1>
                <p class="lead">This is the model tab. You can input and edit tasks, reorder them, import & export a series of tasks, and save the current ordering of tasks. </p>
            </div>
        </div>
        <br>
        <!-- /.row -->
        <div id="my_container" class ="container-fluid">
            <div style="width:100px;height:auto;float:left">
                <br> <br> <br> <!-- TODO: this is a cheap tactic -->
                <div class="header_rotate" style="font-size:x-large;width:100px;"> <p> Task Steps </p></div>
                <br><br><br><br><br><br><br><br> <!-- TODO: this is still a cheap tactic -->
                <div class="header_rotate" style="font-size:x-large;width:100px"> <p> Skills & Tools </p></div>
            </div>
            <div class="task-steps" style="overflow-x:scroll;overflow-y:visible">
                <div style="width:5000px;overflow-y:visible" class="sortable"> <!-- TODO: this width / overflow thing is a little hacky -->
                <!-- <p> Tasks </p> -->

                <% tasks.forEach( function( task ){ %>
                     <div id="<%= task.id %>" class="task " style="float:left;position:static;left:<%=task.order_number%>"> 
                        <div>
                          <a href="/destroy/<%= task.id %>" title="Delete"><image src="images/delete.png" height="20px"></a>
                          <script> var tasks2 = <%- JSON.stringify(tasks) %> </script>
                          <a onclick="editTask(this, tasks2)" title="Edit" id="<%= task.id %>"><image src="images/edit.png" height="20px"></a>
                        </div>
                        <div style="border-style:solid;background:white">
                            <!-- <p>ID: <%= task.id %> -->
                            <p style="text-align:center;font-size:large"> <%= task.descript %></p>

                            <%if (parents[task._id].length != 0) { %>
                                Parents: <%= parents[task._id] %><br>
                            <% } %> 
                            <%if (task.duration != null && task.duration != "") { %>
                                Duration: <%= task.duration %><br>
                            <% } %>
 <!--                            <%if (task.skills.length != 0 ) { %>
                                Skills: <%= task.skills %><br>
                            <% } %> 
                            <%if (task.tools.length != 0) { %>
                                Tools: <%= task.tools %><br>
                            <% } %>  -->
<!--                             <%if (task.parents.length != 0) { %>
                                Parents: <%= task.parents %><br>
                            <% } %>  -->
                            <%if (task.arm != null && task.arm != "") { %>
                                Arm: <%= task.arm %><br>
                            <% } %> 
                            <%if (task.grasp_effort != null && task.grasp_effort != "" && task.tools.indexOf("grasp_effort") == -1) { %>
                                Grasp Effort: <%= task.grasp_effort %><br>
                            <% } %> 
                            <%if (task.object != null && task.object != "" && task.tools.indexOf("object") == -1) { %>
                                Object: <%= task.object %><br>
                            <% } %> 
                            <%if (task.orientation != null && task.orientation != "" && task.tools.indexOf("orientation") == -1) { %>
                                Orientation: <%= task.orientation %><br>
                            <% } %> 
                            <%if (task.angle != null && task.angle != "" && task.tools.indexOf("angle") == -1) { %>
                                Angle: <%= task.angle %><br>
                            <% } %> 
                            <%if (task.location != null && task.location != "" && task.tools.indexOf("location") == -1) { %>
                                Location: <%= task.location %><br>
                            <% } %> 
                            <%if (task.size != null && task.size != "" && task.tools.indexOf("size") == -1) { %>
                                Size: <%= task.size %> <br>
                            <% } %> <%if (task.relativeX != null && task.relativeX != "" && task.tools.indexOf("relativeX") == -1) { %>
                                RelativeX: <%= task.relativeX %><br>
                            <% } %> <%if (task.relativeY != null && task.relativeY != "" && task.tools.indexOf("relativeY") == -1) { %>
                                RelativeY: <%= task.relativeY %><br>
                            <% } %> <%if (task.relativeZ != null && task.relativeZ != "" && task.tools.indexOf("relativeZ") == -1) { %>
                                RelativeZ: <%= task.relativeZ %><br>
                            <% } %> <%if (task.max_joint_vel != null && task.max_joint_vel != "" && task.tools.indexOf("max_joint_vel") == -1) { %>
                                Max Joint Velocity: <%= task.max_joint_vel %><br>
                            <% } %> <%if (task.order_number != null && task.order_number != "" ) { %>
                                Order Number: <%= task.order_number %><br>
                            <% } %> <%if (task.short_id != null && task.short_id != "") { %>
                                Short ID: <%= task.short_id %><br>
                            <% } %>
                        </p>
                            <!-- Skills: <%= task.skills %><br>Tools: <%= task.tools %><br>Parents: <%= task.parents %><br>Arm: <%= task.arm %><br>Grasp Effort: <%= task.grasp_effort %><br>Object: <%= task.object %><br>Orientation: <%= task.orientation %><br>Angle: <%= task.angle %><br>Position: <%= task.positon %><br>Size: <%= task.size %><br>RelativeX <%= task.relativeX %><br>RelativeY: <%= task.relativeY %><br>RelativeZ: <%= task.relativeZ %></p> -->
                        </div>
                        <div style="position:relative">
                           <table style="width:100%;">
                              <tr >
                                <td style="width:80px;">
                                    <div style="height:70px;">  
                                    </div>
                                    <% task.skills.forEach( function( skill ){ %>
                                        <div class="skills-arrow"> <%= skill %> <%= task[skill] %>
                                        </div>
                                    <% }); %>
                                </td>
                                <td style="width:40px;background:gray">
                                </td> 
                                <td style="width:80px">
                                    <div style="height:90px"> 
                                    </div>
                                    <% task.tools.forEach( function( tool ){ %>
                                        <div class="tools-arrow"> <%= tool %> <%= task[tool] %>
                                        </div>
                                    <% }); %>
                                </td>
                              </tr>
                              
                            </table>
                        </div>
                    </div>
                <% }); %>
                </div>
            </div>
        </div>
        <br>

    

        <br><br><br>
        <form action="/create" method="post" accept-charset="utf-8" style="clear:both" class="form-horizontal col-sm-6" id="myBigForm">
            <div class="form-group"> 
            <!-- Task Description: <br> -->
            <label class="control-label " for="inputError">Task Description</label>
                <select id="task-choice" name="descript" class="form-control" >

                    <script>
                    $.getJSON("json_data/message_lookup.json", function(data) { 
                        for (key in data){
                            document.getElementById("task-choice").innerHTML = document.getElementById("task-choice").innerHTML + "<option value='"+key+"'>"+key+"</option>";
                        }
                    });
                    </script>
                </select>
            </div>
            <div id="arm" class="robo_param form-group">
                <!-- Arm: <br> -->
                <label class="control-label" for="inputError">Arm</label>
                    <select name="arm" class="form-control">
                    <option value='left'> Left</option>
                    <option value='right'> Right</option></select>
            </div>
<!--             <div id="duration" class="robo_param form-group">
                Duration: <br>
                    <input type="text" name="duration" /> <br>
            </div> -->
            <div id="grasp_effort" class="robo_param form-group">
                <label class="control-label" for="inputError">Grasp Effort</label> 
                <span for="inputError" class="form-control-feedback-text"> must be between 0 and 100, inclusive. </span>
                    <input type="number" name="grasp_effort" class="form-control" value="100"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback"> </span> 
            </div>
            <div id="object" class="robo_param form-group">
                <label class="control-label" for="inputError">Object</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="object" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="orientation" class="robo_param form-group">
                <label class="control-label" for="inputError">Orientation</label>
                <span for="inputError" class="form-control-feedback-text"> can be either 0 or 1, for now. </span>
                    <input type="number" name="orientation" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span> 
            </div>
            <div id="angle" class="robo_param form-group">
                <label class="control-label" for="inputError">Angle</label>
                <span for="inputError" class="form-control-feedback-text"> can be 0, 1, or 2 for now. </span>
                    <input type="number" name="angle" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="location" class="robo_param form-group">
                <label class="control-label" for="inputError">Location</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="location" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="size" class="robo_param form-group">
                <label class="control-label" for="inputError">Size</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="size" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="relativeX" class="robo_param form-group">
                <label class="control-label" for="inputError">Relative X</label>
                    <input type="number" name="relativeX" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="relativeY" class="robo_param form-group">
                <label class="control-label" for="inputError">Relative Y</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="relativeY" class="form-control" value="0"/> 
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="relativeZ" class="robo_param form-group">
                <label class="control-label" for="inputError">Relative Z</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="relativeZ" class="form-control" value="0"/>
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div id="max_joint_vel" class="robo_param form-group">
                <label class="control-label" for="inputError">Max Joint Velocity</label>
                <span for="inputError" class="form-control-feedback-text"> is a required field. </span>
                    <input type="number" name="max_joint_vel" class="form-control" value="100"/>
                    <span class="glyphicon glyphicon-remove form-control-feedback" ></span> 
            </div>
            <div class="checkbox">
            <b>Skills</b> <br>
                <label><input type="checkbox" name="skills[]" value="move" > Move </label>
                <br>
                <label><input type="checkbox" name="skills[]" value="rotate" > Rotate </label>
                <br>
            </div>
<!--             <div class="checkbox ">
            <b>Tools</b> <br>
                <label><input type="checkbox" name="tools[]" value="rotator" > Rotator</label>
                <br>
                <label><input type="checkbox" name="tools[]" value="fastener" > Fastener</label>
                <br>
            </div> -->
            <div class="checkbox">
            <b>Parents</b> <br>
                <% tasks.forEach( function( task ){ %>
                    <label><input type="checkbox" name="parents[]" value="<%= task.id %>"> <%= task.descript %> <%= task.short_id %></label>
                    <br>
                <% }); %>
            </div>
            <br>
            <input type="submit" value="Submit">
        </form> 

        <div class="col-sm-4  col-sm-offset-2"> <b>Actions Menu </b>
            <div>
                <a href="/exports" title="Exports">Export All Tasks</a>
            </div>
            <div>
                <a href="/imports" title="Imports">Import All Tasks</a>
            </div>
            <div>
                <form action="/sendtasks" method="post" accept-charset="utf-8" style="clear:both">
                    <input type="submit" value="Send Tasks"> to IP <input type="text" name="ip_destination" id="ip_destination_input" value="128.31.35.87"/> 
                </form>
                <!-- <a href="/sendtasks" title="SendTasks">Send Tasks to Robot</a> -->
            </div>
            <div>
                <a onclick="saveWorkspace()" title="SaveWorkspace">Save Workspace</a>
            </div>

            <br style="clear:both">
            <button onclick="taskInventory()">Get Task Inventory</button>
            <br>

            <div ID="taskInventoryList">
                List starts here
            </div>
            <br> Times for Each Task <br>
            <% times.forEach( function( time ){ %>
                <%= time %> <br>
            <%});%>
        </div>

    </div>
